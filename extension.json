{
	"name": "DBExt2 with 4 db types",
	"version": "0.0.1",
	"author": [
		"Art Baltai"
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:DBExt2",
	"descriptionmsg": "DBExt2 test",
	"type": "database",
	"MessagesDirs": {
		"DBExt2": [
			"i18n"
		]
	},
	"Providers": {
		"Databases": {
			"dbext2type1": {
				"Installer": "DbExt2\\DBExt2Installer1",
				"Updater": "DbExt2\\DBExt2Updater1",
				"Driver": "DbExt2\\DBExt2Driver1"
			},
			"dbext2type2": {
				"Installer": "DbExt2\\DBExt2Installer2",
				"Updater": "DbExt2\\DBExt2Updater2",
				"Driver": "DbExt2\\DBExt2Driver2"
			},
			"dbext2type4": {
				"Installer": "DbExt2\\DBExt2Installer4",
				"Updater": "DbExt2\\DBExt2Updater4",
				"Driver": "DbExt2\\DBExt2Driver4"
			}
		}
	},
	"AutoloadClasses": {
		"DbExt2\\DBExt2Installer1": "includes/DBExt2Installer1.php",
		"DbExt2\\DBExt2Installer2": "includes/DBExt2Installer2.php",
		"DbExt2\\DBExt2Installer3": "includes/DBExt2Installer3.php",
		"DbExt2\\DBExt2Installer4": "includes/DBExt2Installer4.php",
		"DbExt2\\DBExt2Updater1": "includes/DBExt2Updater1.php",
		"DbExt2\\DBExt2Updater2": "includes/DBExt2Updater2.php",
		"DbExt2\\DBExt2Updater3": "includes/DBExt2Updater3.php",
		"DbExt2\\DBExt2Updater4": "includes/DBExt2Updater4.php",
		"DbExt2\\DBExt2Driver1": "includes/DBExt2Driver1.php",
		"DbExt2\\DBExt2Driver2": "includes/DBExt2Driver2.php",
		"DbExt2\\DBExt2Driver4": "includes/DBExt2Driver4.php"

	},
	"manifest_version": 1
}

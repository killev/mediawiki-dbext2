{
	"name": "DBExt2 with 4 db types",
	"version": "0.0.1",
	"author": [
		"Art Baltai"
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:DBExt2",
	"descriptionmsg": "DBExt2 test",
	"type": "database",
	"MessagesDirs": {
		"DBExt2": [
			"i18n"
		]
	},
	"Providers": {
		"Databases": {
			"dbext2type1": {
				"Installer": "DbExt2\\DBExt2Installer1"
			},
			"dbext2type2": {
				"Installer": "DbExt2\\DBExt2Installer2"
			},
			"dbext2type3": {
				"Installer": "DbExt2\\DBExt2Installer3"
			},
			"dbext2type4": {
				"Installer": "DbExt2\\DBExt2Installer4"
			}
		}
	},
	"AutoloadClasses": {
		"DbExt2\\DBExt2Installer1": "includes/DBExt2Installer1.php",
		"DbExt2\\DBExt2Installer2": "includes/DBExt2Installer2.php",
		"DbExt2\\DBExt2Installer3": "includes/DBExt2Installer3.php",
		"DbExt2\\DBExt2Installer4": "includes/DBExt2Installer4.php"
	},
	"manifest_version": 1
}
